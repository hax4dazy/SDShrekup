@page "/switch"
@inject Microsoft.AspNetCore.Components.NavigationManager UriHelper

@functions {
	protected override async Task OnInitializedAsync() {
		string extension = "";
		if (UriHelper.Uri.Contains("#")) extension = "#" + UriHelper.Uri.Split('#').Last();
		UriHelper.NavigateTo("console?switch" + extension);
	}
}